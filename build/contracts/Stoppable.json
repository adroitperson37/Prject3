  "source": "pragma solidity ^0.4.17;\n\n\nimport \"./Ownable.sol\";\n\n\n/**\n * The contractName contract does this and that...\n */\ncontract Stoppable is Ownable {\n\n    bool public isOn;\n\n    function Stoppable () public {\n        isOn = true;\n    }\n\n    function startOrStop (bool on)  public onlyOwner returns(bool success) {\n        isOn = on;\n        return true;\n    }\n\n    modifier isRunning() {\n        require(isOn == true);\n        _;\n    }\n\n}\n",
